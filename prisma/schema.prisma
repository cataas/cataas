generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Cat {
    id String @id @default(uuid())
    tags String[]
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
    file String
    mimetype String
    size Int
    validated Boolean @default(false)
}
